# This config file routes the ssh to correct host/server - Debashis Paul
# Default GitHub (send2deb account)
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_send2deb

# GitHub account for admin@debdroid.com 
Host github-debdroid
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_debdroid

# GitHub account for pdroid84 
Host github-pdroid84
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa

# Bluehost account for debdrwxu 
Host bluehost
  HostName 162.215.253.110
  Port 22
  User debdrwxu
  IdentityFile ~/.ssh/id_rsa_bluehost